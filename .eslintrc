{
    "extends": "eslint-config-google",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "plugins": [
        "babel"
    ],
    "env": {
        "es6": true
    },
    "rules": {
        "require-jsdoc": "off",
        "max-len": "off",
        "new-cap": "off",
        "no-extend-native": "off",
        "spaced-comment": "off"
    }
}
