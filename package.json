{
  "name": "cafeteria-server",
  "type": "module",
  "main": "./index.js",
  "version": "1.0.0",
  "description": "Server side application for INU Cafeteria.",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "devstart": "node dist/index.js",
    "prestart": "sudo systemctl enable cafeteria",
    "start": "sudo systemctl start cafeteria",
    "restart": "sudo systemctl restart cafeteria",
    "stop": "sudo systemctl stop cafeteria",
    "pretest": "touch lib/config/config.mjs",
    "test": "jest --runInBand --detectOpenHandles",
    "unittest": "touch lib/config/config.mjs && jest --runInBand --detectOpenHandles test/unit",
    "status": "sudo systemctl status cafeteria && echo 'Server is running.' || echo 'Server is not running.'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inu-appcenter/cafeteria-server.git"
  },
  "keywords": [
    "inu",
    "cafeteria"
  ],
  "author": "potados",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/inu-appcenter/cafeteria-server/issues"
  },
  "homepage": "https://github.com/inu-appcenter/cafeteria-server#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@hapi/boom": "^9.0.0",
    "@hapi/good": "^9.0.0",
    "@hapi/good-console": "^8.1.2",
    "@hapi/good-squeeze": "^5.2.1",
    "@hapi/hapi": "^19.1.1",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.0",
    "@hapi/vision": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "bcrypt": "^4.0.0",
    "blipp": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-babel": "^5.3.0",
    "hapi-auth-jwt2": "^9.0.1",
    "hapi-swagger": "^12.1.1",
    "jest": "^25.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.0",
    "npm": "^6.14.2",
    "rand-token": "^0.4.0",
    "sequelize": "^5.21.5",
    "stack-trace": "0.0.10",
    "stream": "0.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "_moduleAliases": {
    "root": ".",
    "config": "lib/config",
    "common": "lib/common",
    "domain": "lib/domain",
    "interfaces": "lib/interfaces",
    "infrastructure": "lib/infrastructure"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  },
  "devDependencies": {
    "eslint-plugin-class-property": "^1.1.0"
  }
}
